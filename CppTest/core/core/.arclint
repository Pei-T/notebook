{
  "linters": {
    "c": {
      "type": "cpplint",
      "flags": [
        "--extensions=h,c"
      ],
      "include": [
        "(\\.h$)",
        "(\\.c$)"
      ],
      "exclude": [
        "(core/include/core/result.h)",
        "(core/include/core/optional.h)",
        "(core/include/core/string_view.h)"
      ],
      "severity": {
        "whitespace/tab": "disabled",
        "build/include_subdir": "disabled",
        "runtime/references": "disabled"
      }
    },
    "c++": {
      "type": "cpplint",
      "include": [
        "(\\.h$)",
        "(\\.cc$)",
        "(\\.cpp$)"
      ],
      "exclude": [
        "(core/include/core/result.h)",
        "(core/include/core/optional.h)",
        "(core/include/core/string_view.h)"
      ],
      "severity": {
        "build/c++11": "disabled",
        "whitespace/tab": "disabled",
        "build/include_subdir": "disabled",
        "runtime/references": "disabled"
      }
    }
  }
}